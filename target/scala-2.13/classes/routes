# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(file)

# Login-page
GET     /login                      controllers.HomeController.login

# Signup-page
GET     /signup                      controllers.HomeController.signup

# About-page
GET     /about                      controllers.HomeController.about

# tasklist-page
GET     /tasklist                      controllers.HomeController.tasklist

# userlogin

POST     /userlogin                      controllers.HomeController.userlogin

# createUser

POST     /createUser                       controllers.HomeController.createUser

# logout
GET     /logout                              controllers.HomeController.logout

# add task
POST     /addTask                              controllers.HomeController.addTask
